<h1>Add a movie</h1>
<%= form_tag "/movies" do %>
  <p>
    <%= label_tag :director %><br>
    <%= text_field_tag :director %>
  </p>
  <p>
    <%= label_tag :title %><br>
    <%= text_field_tag :title %>
  </p>
  <p>
    <%= label_tag :url %><br>
    <%= text_field_tag :url %>
  </p>
  <p>
    <%= submit_tag "Save" %>
  </p>
<% end %>